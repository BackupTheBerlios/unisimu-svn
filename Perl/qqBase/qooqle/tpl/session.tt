[%- is_odd = 1 %]
[%- session = 0 %]
[%- first_time = 1 %]
[%- FOREACH r = hits %]
    [%- IF !session OR r.session != session %]

        [%- session = r.session %]
        [%- IF first_time %]
            [%- first_time = 0 %]
        [%- ELSE %]
  </table>

  <p>
  <hr>
  <p>
        [%- END %]

  <font color="#6472b6"><b><I>[% r.session_time %]</I></b></font>
  <br><br>

  <table width=100%>
  <tr><th>Who<th>What<th>When
        [%- is_odd = 0 %]
    [%- END %]

  <tr class="[% is_odd ? 'odd' : 'even' %]">
  <td align=center>
  <font size=-1>[% r.sender %]</font></td>
  <td width=70% align=left>
      <font size=-1>
      [%- msg = r.message %]
      [%- msg = msg.replace("<", "\&lt;") %]
      [%- msg = msg.replace(">", "\&gt;") %]
      [%- msg = msg.replace("\n", "<br>\n") %]
      [%- msg = msg.replace(" ", "\&nbsp;") %]
      [%- msg.replace("\t", "\&nbsp;\&nbsp;\&nbsp;\&nbsp;") %]
      </font>
  </td>
  <td align=center><font color=blue>[% r.msg_time %]</font></td>
  </tr>
    [%- is_odd = is_odd ? 0 : 1 %]
[% END %]
</table>

